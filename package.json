{
  "name": "auth-server",
  "version": "1.0.0",
  "description": "Auth Server",
  "license": "MIT",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:prod": "node dist/main.js",
    "lint": "tslint --project tsconfig.json --fix",
    "build": "./node_modules/.bin/tsc -p tsconfig.json",
    "test" : "cross-env NODE_ENV=test node_modules/.bin/jest --colors --config=./jest-global.config.js --runInBand --coverage",
    "test:unit": "cross-env NODE_ENV=test node_modules/.bin/jest --colors --config=./jest.config.js --runInBand --coverage",
    "test:e2e": "cross-env NODE_ENV=test node_modules/.bin/jest --colors --config=./jest-e2e.config.js --runInBand --coverage"
  },
  "dependencies": {
    "@nestjs/common": "^5.0.0",
    "@nestjs/core": "^5.0.0",
    "@nestjs/passport": "^1.1.0",
    "@nestjs/swagger": "^2.2.0",
    "@nestjs/typeorm": "^5.1.0",
    "bcrypt": "2.0.0",
    "class-transformer": "^0.1.9",
    "class-validator": "^0.9.1",
    "cross-env": "^5.2.0",
    "elasticsearch": "^14.2.2",
    "es-mapping-ts": "0.0.8",
    "fastify-formbody": "^2.0.0",
    "json-override": "^0.2.0",
    "jsonwebtoken": "^8.3.0",
    "mongodb": "^3.1.1",
    "lodash": "^4.17.5",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "password": "^0.1.1",
    "typeorm": "^0.2.7",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "swagger-express-ts": "^1.0.0",
    "swagger-ui-dist": "^3.10.0",
    "typescript": "^2.6.2",
    "url-join": "^4.0.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^2.0.0",
    "@nestjs/testing": "^5.0.0",
    "@types/express": "^4.0.39",
    "@types/jest": "^21.1.8",
    "@types/node": "^9.3.0",
    "@types/supertest": "^2.0.4",
    "copyfiles": "^1.2.0",
    "jest": "^21.2.1",
    "nodemon": "^1.14.1",
    "prettier": "^1.11.1",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.4",
    "ts-loader": "^4.1.0",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^3.1.1",
    "tsconfig-paths-jest": "0.0.1",
    "tslint": "5.3.2",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  },
  "repository": {
    "type": "git",
    "url": "git@cdsgit.asi-rennes.fr:Inexys/inexys-auth.git"
  }
}
